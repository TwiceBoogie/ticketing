{
  "repository": "https://github.com/TwiceBoogie/ticketing.git",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "auth",
    "expiration",
    "orders",
    "payments",
    "tickets",
    "web"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev --concurrency=13",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "lint:errors": "turbo run lint:errors",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0"
  },
  "packageManager": "yarn@4.9.1",
  "name": "twicetickets"
}
